{
  "name": "powerjudge-api-server",
  "version": "0.0.1",
  "description": "Online judge server",
  "keywords": [
    "judge",
    "judge-server",
    "judge-system"
  ],
  "bin": {
    "powerjudge-api-server": "./dist/src/bin/powerjudge-api-server.js"
  },
  "main": "./dist/src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all clean build gulp:copy-files server",
    "start:dev": "npm-run-all clean gulp:copy-files --parallel watch:build watch:server --print-label",
    "clean": "rimraf dist",
    "build": "tsc",
    "gulp:copy-files": "node_modules/.bin/gulp copy-files",
    "server": "node ./dist/src/main.js",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon -- run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/powerumc/powerjudge-server.git"
  },
  "author": "powerumc",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/powerumc/powerjudge-server/issues"
  },
  "homepage": "https://github.com/powerumc/powerjudge-server#readme",
  "dependencies": {
    "@microsoft/ts-command-line": "^4.2.3",
    "command-exists": "^1.2.8",
    "debug": "^4.1.1",
    "dockerode": "^2.5.8",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "rxjs": "^6.2.2",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "3.8.0",
    "src": "latest"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^10.12.24",
    "@types/gulp": "^4.0.6",
    "@types/command-exists": "^1.2.0",
    "@types/dockerode": "^2.5.12",
    "@nestjs/testing": "^5.1.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3",
    "ts-node": "^8.0.3",
    "gulp": "^4.0.0",
    "tsconfig-paths": "file:local_modules/tsconfig-paths"
  }
}
